<!--packages start-->
<section id="tabs" class="project-tab">
  <div class="container">
    <div class="row">
      <div class="col-md-12 mt-10">
        <h3 class="title"> Static chart: Booking Tours/ Month</h3>
        <%= line_chart BookingTour.where("status <> 1").group_by_month_of_year(:created_at, format: "%b").count %>
      </div>
    </div>
  </div>
</section>

<section id="tabs" class="project-tab">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h3 class="title mb-10 mt-10"> Static chart: Reviews/ Month(Only staticics for review is not status reject)</h3>
        <%= line_chart Review.where("status <> 1").group_by_month_of_year(:created_at, format: "%b").count %>
      </div>
    </div>
  </div>
</section>